document.addEventListener('DOMContentLoaded',function(){
    var pathname = location.pathname;
    var star = 0;
    for (var i = 0; i < pathname.length; i++) {
        star = parseInt(star) + parseInt(pathname.charCodeAt(i));
    }
    var str = '';
    if (star % 5 < 2) {
        for (i = 0; i < 4; i++) {
            str += '<img src="'+ currentPath +'img/icon_star.png" alt="">\n';
        }
        str += '<img src="data:image/png;base64,">'

    } else {
        for (i = 0; i < 5; i++) {
            str += '<img src="'+ currentPath +'img/icon_star.png" alt="">\n';
        }
    }
    var lala = '//' + location.host;
    var r = Math.random();
    var d = '<a href="' + lala + '/dldoc/index.html?url=' + pathname + '?r=' + r +'" target="_blank" class="download_card">\n' +
        '        <img class="download_card_pic" src="'+ currentPath +'img/icon_word.png" alt="">\n' +
        '        <div class="download_card_msg">\n' +
        '            <div class="download_card_title" style="text-decoration:none;">《' + document.title + '.doc》</div>\n' +
        '            <div class="download_card_tip">将本文的Word文档下载到电脑，方便收藏和打印</div>\n' +
        '            <div class="download_card_tj">\n' +
        '                <span>推荐度：</span>\n' + str +
        '            </div>\n' +
        '        </div>\n' +
        '        <div class="download_card_btn">\n' +
        '            <img src="data:image/svg+xml;base64," alt="">\n' +
        '            <div class="downlod_btn_right">\n' +
        '                <div>点击下载文档</div>\n' +
        '                <p>文档为doc格式</p>\n' +
        '            </div>\n' +
        '        </div>\n' +
        '    </a>';
    $(document).on('click','.download_card',function (e) {
        var reg = /^(m\.).*/;
        if(reg.test(location.host)){
            $(this).attr('href',lala + '/dldoc/index' + pathname + '?r=' + r + timeNum);
        }else{
            $(this).attr('href',lala + '/dldoc/index.html?url=' + pathname + '?r=' + r + timeNum);
        }
    });
    var flag = true;
    var bmd = ['/jianlixiazai/','/company/','/zhaopin/','/mingqi/','/gaoxiao/','/sydwzp/','/xuanjianghui/','/zhaopinhui/','/ftp/','ask','/zi/','/ci/','/yy/','/my/','/xhy/','/chengyu/','/zuci/','/zj/'];
    var reg = /\d+\.(htm)/
    for(var i = 0; i < bmd.length;i++){
        if(location.href.indexOf(bmd[i]) != -1 || !reg.test(location.href)){
            flag = false;
            break;
        }
    }
    if ($(".content") && $(".content").text().replace(/\s/g,'').length > 700 && flag) {
        $(".content").after(d);
    }
});
